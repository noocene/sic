true = \t \f
    t

false = \t \f
    f

not = \b \t \f
    (b f t)

0 = \s
    : S = s
    . \z z

1 = \s
    : S = s
    . \z (S z)

add = \a \b \s
    : S = s
    : A = (a .S)
    : B = (b .S)
    . \z (A (B z))

mul = \a \b \s
    : S = s
    (a (b .S))

2 = (add 1 1)
3 = (add 2 1)

main = (mul 3 2)